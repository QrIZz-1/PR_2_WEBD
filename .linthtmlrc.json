{
	"maxerr": false,
	"text-ignore-regex": false,
	"raw-ignore-regex": false,
	"attr-name-ignore-regex": false,
	"id-class-ignore-regex": false,
	"line-max-len-ignore-regex": false,
	"rules": {
		"attr-bans": [
			true,
			[
			  "align",
			  "background",
			  "bgcolor",
			  "border",
			  "clear",
			  "frameborder",
			  "longdesc",
			  "marginwidth",
			  "marginheight",
			  "scrolling",
			  "style",
			  "width",
			  "height",
			  "hspace",
			  "link",
			  "nowrap",
			  "vlink",
			  "vspace"
			]
		  ],
		  "attr-name-style": [
			true,
			"dash"
		  ],
		  "attr-no-dup": true,
		  "attr-no-unsafe-char": true,
		  "attr-order": [
			true,
			[
			  "class",
			  "id",
			  "name",
			  "data-.+",
			  "src",
			  "for",
			  "type",
			  "href",
			  "value",
			  "title",
			  "alt",
			  "role",
			  "aria-.+"
			]
		  ],
		  "attr-quote-style": [
			true,
			"double"
		  ],
		  "attr-req-value": true,
		  "attr-validate": true,
		  "class-no-dup": true,
		  "class-style": [
			true,
			"lowercase",
			"dash"
		  ],
		  "doctype-first": [
			true,
			"smart"
		  ],
		  "doctype-html5": true,
		  "fieldset-contains-legend": true,
		  "fig-req-figcaption": true,
		  "focusable-tabindex-style": true,
		  "head-req-title": true,
		  "head-valid-content-model": true,
		  "html-req-lang": true,
		  "html-valid-content-model": true,
		  "id-no-dup": true,
		  "id-style": [
			true,
			"lowercase",
			"dash"
		  ],
		  "img-req-alt": true,
		  "img-req-src": true,
		  "indent-style": [
			true,
			"tabs"
		  ],
		  "indent-width": [
			true,
			1
		  ],
		  "input-btn-req-value-or-title": true,
		  "input-radio-req-name": true,
		  "label-no-enc-textarea-or-select": true,
		  "label-req-for": true,
		  "lang-style": true,
		  "line-end-style": [
			true,
			"lf"
		  ],
		  "link-min-length-4": true,
		  "line-no-trailing-whitespace": true,
		  "link-req-noopener": true,
		  "spec-char-escape": true,
		  "table-req-caption": true,
		  "table-req-header": true,
		  "tag-bans": [
			true,
			[
			  "acronym",
			  "applet",
			  "basefont",
			  "big",
			  "center",
			  "dir",
			  "font",
			  "frame",
			  "frameset",
			  "listing",
			  "isindex",
			  "noframes",
			  "s",
			  "strike",
			  "tt",
			  "u",
			  "b",
			  "embed",
			  "menu",
			  "plaintext",
			  "xmp"
			]
		  ],
		  "tag-close": true,
		  "tag-name-lowercase": true,
		  "tag-name-match": true,
		  "tag-self-close": [
			true,
			"never"
		  ],
		  "title-no-dup": true,
		  "title-max-len": [
			true,
			60
		  ]
		}
	}
